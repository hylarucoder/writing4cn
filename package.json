{
    "name": "writing4cn",
    "displayName": "writing4cn",
    "description": "为什么你们就不能格式化一下你们的 Markdown 呢",
    "version": "0.0.2",
    "publisher": "twocucao",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.reformatSelection4cn"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.reformatSelection4cn",
            "title": "Writing4CN : Pangu Reformat"
        }]
